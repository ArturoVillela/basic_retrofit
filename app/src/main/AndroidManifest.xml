<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="charlieandroidblog.esy.es.testaltimetrik">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectUseAppActivity" />
        <activity android:name=".Activities.User.UserActivity" />
        <activity android:name=".Activities.Agency.AgencyActivity" />
        <activity android:name=".Activities.Status.StatusActivity" />
        <activity android:name=".LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SelectionActivity.SelectionActivity"></activity>
    </application>

</manifest>